{
  "name": "Megaton Backend",
  "version": "1.0.0",
  "description": "Rest API using Node.js, TypeScript ORM Sequelize, Express, Postgres",
  "main": "server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "node ./build/server.js",
    "start": "tsc && npm run dev",
    "db:create": "sequelize-cli-typescript db:create",
    "db:migrate": "sequelize-cli-typescript db:migrate",
    "db:seed:all": "sequelize-cli-typescript db:seed:all"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "orm",
    "sequelize",
    "express",
    "postgres",
    "postgresql",
    "restapi",
    "rest",
    "api",
    "crud"
  ],
  "author": "Megaton",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "sequelize-cli-typescript": "^3.2.0-c",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "pg": "^8.11.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.6"
  }
}
